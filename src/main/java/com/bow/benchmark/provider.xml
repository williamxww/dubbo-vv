<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="provider" logger="slf4j"/>
    <dubbo:registry address="zookeeper://10.170.177.119:2181"/>
    <dubbo:protocol  name="dubbo" port="20880" />
    <dubbo:protocol  name="rest" port="9000" />


    <bean id="benchmarkServiceImpl" class="com.bow.service.impl.BenchmarkServiceImpl"></bean>
    <dubbo:service protocol="rest" interface="com.bow.service.BenchmarkService" ref="benchmarkServiceImpl"/>
</beans>